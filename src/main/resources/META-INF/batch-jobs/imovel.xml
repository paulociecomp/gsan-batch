<job id="imovel" xmlns="http://xmlns.jcp.org/xml/ns/javaee" version="1.0">
	<step id="processar_dados_diarios">
		<chunk checkpoint-policy="item" item-count="50">
			<reader ref="processoImovelInicio">
				<properties>
					<property name="primeiroItem" value="#{partitionPlan['primeiroItem']}" />
					<property name="numItens" value="#{partitionPlan['numItens']}" />
				</properties>
			</reader>
			<processor ref="processoImovel" />
			<writer ref="processoImovelFim" />
		</chunk>
		<partition>
			<mapper ref="gerarDadosParaLeituraParticionador" />
		</partition>
	</step>
</job>

